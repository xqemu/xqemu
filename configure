#include <OpenGL/CGLCurrent.h>
int main(void) { return GL_VERSION != 0; }
EOF
  elif test "$mingw32" = "yes" ; then
	opengl_libs="-lopengl32 -lglew32 -lgdi32"
	cat > $TMPC << EOF
#include <windows.h>
#include <GL/glew.h>
#include <GL/gl.h>
#include <GL/glext.h>
#include <GL/wglext.h>
int main(void) { return GL_VERSION != 0; }
EOF
  else
    opengl_libs="-lGLEW -lGLU -lGL -lX11"
    cat > $TMPC << EOF
#include <GL/glew.h>
#include <GL/glu.h>
#include <GL/glx.h>
#include <X11/Xlib.h>
int main(void) { glXGetClientString(XOpenDisplay(0), GL_VERSION); return 0; }
EOF
  fi
  if compile_prog "" "$opengl_libs" ; then
    opengl=yes
    libs_softmmu="$opengl_libs $libs_softmmu"
  else
    if test "$opengl" = "yes" ; then
      feature_not_found "opengl"
    fi
    opengl_libs=
    opengl=no
  fi
fi

